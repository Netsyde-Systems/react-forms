{
  "name": "@netsyde/react-forms",
  "description": "A Small, Simple, Strongly Typed Forms Library",
  "author": "Miro Koprnicky", 
  "keywords": [
    "React",
    "Forms",
    "TypeScript"
  ],
  "homepage": "https://netsyde-systems.github.io/react-forms/",
  "license": "MIT",
  "version": "0.6.21",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Netsyde-Systems/react-forms.git"
  },
  "bugs": {
    "url": "https://github.com/Netsyde-Systems/react-forms/issues"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "vite --open",
    "test": "vitest --environment=jsdom",
    "coverage": "vitest run --coverage",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "deploy": "npm run build && gh-pages -d build",
    "resetdeps": "rimraf package-lock.json && rimraf node_modules/ && npm install",
    "cleanlib": "rimraf lib/",
    "copystyles": "copyfiles -u 1 src/**/*.scss lib/",
    "compile": "npm run cleanlib && npm run copystyles && tsc --project tsconfig.lib.json",
    "precommit": "npm run compile",
    "postcommit": "npm publish && npm run deploy"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "date-fns": "^2.29.3",
    "deep-equal": "^2.2.2",
    "email-validator": "^2.0.4",
    "react": "^18.2.0",
    "react-datepicker": "^4.8.0",
    "react-dom": "^18.2.0",
    "react-imask": "^6.6.1",
    "sass": "^1.52.3"
  },
  "main": "lib/index",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/deep-equal": "^1.0.1",
    "@types/react": "^18.2.15",
    "@types/react-datepicker": "^4.11.2",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "copyfiles": "^2.4.1",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "gh-pages": "^5.0.0",
    "jsdom": "^22.1.0",
    "rimraf": "^5.0.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vitest": "^0.33.0"
  }
}
