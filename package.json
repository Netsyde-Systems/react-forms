{
  "name": "react-forms",
  "description": "A Small, Simple, Strongly Typed Forms Library",
  "keywords": [
    "React",
    "Forms",
    "TypeScript"
  ],
  "homepage": "https://netsyde-systems.github.io/react-forms/",
  "license": "MIT",
  "version": "0.2.9",
  "repository": {
    "type": "git",
    "url": "https://github.com/Netsyde-Systems/react-forms"
  },
  "bugs": {
    "url": "https://github.com/Netsyde-Systems/react-forms/issues"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && npm run create404",
    "create404": "shx cp build/index.html build/404.html",
    "test": "react-scripts test",
    "coverage": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "deploy": "npm run build && gh-pages -d build",
    "cleanlib": "rimraf lib/",
    "copystyles": "copyfiles -u 1 src/**/*.scss lib/",
    "compilelib": "npm run cleanlib && npm run copystyles && tsc --project tsconfig.lib.json"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "email-validator": "^2.0.4",
    "imask": "^6.4.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "sass": "^1.52.3"
  },
  "main": "lib/indexExports",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.3.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.39",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "copyfiles": "^2.4.1",
    "gh-pages": "^4.0.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "rimraf": "^3.0.2",
    "shx": "^0.3.4",
    "typescript": "^4.7.3",
    "web-vitals": "^2.1.4"
  }
}
